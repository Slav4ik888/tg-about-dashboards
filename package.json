    "telegraf": "4.16.3"
  },
  "devDependencies": {
    "@types/jest": "30.0.0",
    "@types/webpack": "5.28.5",
    "@types/webpack-dev-server": "4.7.1",
    "concurrently": "9.2.1",
    "copyfiles": "2.4.1",
    "cross-env": "^10.0.0",
    "jest": "30.2.0",
    "nodemon": "^3.1.10",
    "ts-jest": "29.4.5",
    "ts-loader": "9.5.4",
    "ts-node": "10.9.2",
