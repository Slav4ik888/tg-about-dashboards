{
  "name": "tg-about_dashboards_bot",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "build": "concurrently \"npm run copy\" \"npm run copy-env\" \"npm run tsc\"",
    "tsc": "tsc -p tsconfig.prod.json",
    "copy": "copyfiles -u 1 src/data/images/**/* env build",
    "copy-env": "copyfiles .env build",
    "dev": "cross-env NODE_ENV=development nodemon ./build/src/index.js",
    "start": "cross-env NODE_ENV=production node ./src/index.js",
    "test:unit": "jest --config ./config/jest/jest.config.ts"
  },
  "keywords": [],
